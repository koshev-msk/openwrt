#include "mt7628an.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>

/ {
	compatible = "zbtlink,zbt-we826-t3", "mediatek,mt7628an-soc";

	aliases {
		led-boot = &led_wlan;
		led-failsafe = &led_wlan;
		led-running = &led_wlan;
		led-upgrade = &led_wlan;
	};
	
	chosen {
		bootargs = "console=ttyS0,115200";
	};

	keys {
		compatible = "gpio-keys-polled";
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio 38 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led_wlan: wlan {
			label = "blue:wlan";
			gpios = <&gpio 44 GPIO_ACTIVE_LOW>;
			linux-default-trigger = "phy0pt";
		};

		wan {
			label = "blue:wan";
			gpios = <&gpio 39 GPIO_ACTIVE_LOW>;
		};

		lan1 {
			label = "blue:lan1";
			gpios = <&gpio 40 GPIO_ACTIVE_LOW>;
		};

		lan2 {
			label = "blue:lan2";
			gpios = <&gpio 41 GPIO_ACTIVE_LOW>;
		};

		lan3 {
			label = "blue:lan3";
			gpios = <&gpio 42 GPIO_ACTIVE_LOW>;
		};

		lan4 {
			label = "blue:lan4";
			gpios = <&gpio 43 GPIO_ACTIVE_LOW>;
		};
	};

	// Maybe unused
	/*
	watchdog {
		compatible = "linux,wdt-gpio";
		gpios = <&gpio 37 GPIO_ACTIVE_LOW>;
		hw_algo = "toggle";
		// hw_margin_ms is actually ~120s but driver limits it to 60s
		hw_margin_ms = <60000>;
		always-running;
	};
	*/

	// mPCIe modem power
	gpio-export {
		compatible = "gpio-export";
		#size-cells = <0>;

		modem {
			gpio-export,name = "modem";
			gpio-export,output = <1>;
			gpios = <&gpio 1 GPIO_ACTIVE_HIGH>;
		};
	};
};

&pcie {
	status = "okay";
};


&spi0 {
	status = "okay";

	m25p80@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <10000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0x30000>;
				read-only;
			};

			partition@30000 {
				label = "u-boot-env";
				reg = <0x30000 0x10000>;
				read-only;
			};

			partition@40000 {
				label = "factory";
				reg = <0x40000 0x10000>;
				read-only;

				nvmem-layout {
					compatible = "fixed-layout";
					#address-cells = <1>;
					#size-cells = <1>;

					eeprom_factory_0: eeprom@0 {
						reg = <0x0 0x400>;
					};

					macaddr_factory_4: macaddr@4 {
						reg = <0x4 0x6>;
					};
				};
			};

			firmware: partition@50000 {
				compatible = "denx,uimage";
				label = "firmware";
			};
		};
	};
};

&ethernet {
	nvmem-cells = <&macaddr_factory_4>;
	nvmem-cell-names = "mac-address";
};

&wmac {
	status = "okay";
	nvmem-cells = <&eeprom_factory_0>;
	nvmem-cell-names = "eeprom";
	
};

&sdhci {
        status = "okay";
        pinctrl-0 = <&sdxc_router_pins>;
};

// SDHC Card use UART1 I2C and I2S lines
&pinctrl {
	ephy-analog;
	
	sdxc_router_pins: sdxc_router_mode {
		gpio {
			groups = "esd", "i2c", "i2s", "sdmode", "uart1", "gpio";
			function = "gpio";
		};

	};
};

&state_default {
	gpio {
		groups = "p0led_an", "p1led_an", "p2led_an", "p3led_an", "p4led_an", "wdt", "wled_an";
		function = "gpio";
	};
};


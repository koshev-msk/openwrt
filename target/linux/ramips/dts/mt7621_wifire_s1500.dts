/dts-v1/;

#include "mt7621.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "wifire,s1500", "mediatek,mt7621-soc";
	model = "Wifire S1500";

	aliases {
		led-boot = &led_status_white;
		led-failsafe = &led_status_white;
		led-running = &led_status_blue;
		led-upgrade = &led_status_white;
		label-mac-device = &gmac0;
	};

	chosen {
		bootargs = "console=ttyS0,115200n8";
	};

	leds {
		compatible = "gpio-leds";

		led_status_blue: status_blue {
			label = "blue:status";
			gpios = <&gpio 29 GPIO_ACTIVE_LOW>;
		};

		led_status_white: status_white {
			label = "white:status";
			gpios = <&gpio 30 GPIO_ACTIVE_LOW>;
			trigger-sources = <&xhci_ehci_port1>, <&ehci_port2>;
			linux,default-trigger = "usbport";
		};

		wan_amber {
			label = "amber:wan";
			gpios = <&gpio 24 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "dsa-0.0:04:link";
		};

		wan_green {
			label = "green:wan";
			gpios = <&gpio 25 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "dsa-0.0:04:1Gbps";
		};

		lan1_amber {
			label = "amber:lan1";
			gpios = <&gpio 22 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "dsa-0.0:03:link";
		};


		lan1_green {
			label = "green:lan1";
			gpios = <&gpio 23 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "dsa-0.0:03:1Gbps";
		};

		lan2_amber {
			label = "amber:lan2";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "dsa-0.0:02:link";
		};

		lan2_green {
			label = "green:lan2";
			gpios = <&gpio 26 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "dsa-0.0:02:1Gbps";
		};

		lan3_amber {
			label = "amber:lan3";
			gpios = <&gpio 10 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "dsa-0.0:01:link";
		};

		lan3_green {
			label = "green:lan3";
			gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "dsa-0.0:01:1Gbps";
		};

		lan4_amber {
			label = "amber:lan4";
			gpios = <&gpio 3 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "dsa-0.0:00:link";
		};

		lan4_green {
			label = "green:lan4";
			gpios = <&gpio 4 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "dsa-0.0:00:1Gbps";
		};

		wlan2g {
			label = "white:wlan2g";
			gpios = <&gpio 28 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy1tpt";
		};

		wlan5g {
			label = "white:wlan5g";
			gpios = <&gpio 32 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};
	};

	keys {
		compatible = "gpio-keys";

		wps {
			label = "wps";
			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};

		reset {
			label = "reset";
			gpios = <&gpio 7 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};
};

&nand {
	status = "okay";

	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		partition@0 {
			label = "Bootloader";
			reg = <0x0 0x80000>;
			read-only;
		};

		partition@80000 {
			label = "Config";
			reg = <0x80000 0x40000>;
		};

		partition@c0000 {
			label = "Bdata";
			reg = <0xc0000 0x40000>;
			read-only;
		};

		factory: partition@100000 {
			label = "factory";
			reg = <0x100000 0x40000>;
			read-only;
		};

		partition@140000 {
			label = "crash";
			reg = <0x140000 0x40000>;
			read-only;
		};

		partition@180000 {
			label = "crash_syslog";
			reg = <0x180000 0x40000>;
			read-only;
		};

		partition@1c0000 {
			label = "reserved0";
			reg = <0x1c0000 0x40000>;
			read-only;
		};

		/* We not keep stock original firmware (kernel0) here */
		partition@200000 {
			label = "kernel_stock";
			reg = <0x200000 0x400000>;
			read-only;
		};

		partition@600000 {
			label = "kernel";
			reg = <0x600000 0x400000>;
		};

		partition@a00000 {
			label = "ubi";
			reg = <0xa00000 0x7580000>;
		};

		partition@ff80000 {
			label = "bbt";
			reg = <0x7f80000 0x80000>;
			read-only;
		};
	};
};

&pcie {
	status = "okay";
};

&pcie0 {
	wifi@0,0 {
		compatible = "mediatek,mt76";
		reg = <0x0000 0 0 0 0>;
		mediatek,mtd-eeprom = <&factory 0x8000>;
		mtd-mac-address = <&factory 0x1000>;
		mtd-mac-address-increment = <3>;
		ieee80211-freq-limit = <5000000 6000000>;
	};
};

&pcie1 {
	wifi@0,0 {
		compatible = "mediatek,mt76";
		reg = <0x0000 0 0 0 0>;
		mediatek,mtd-eeprom = <&factory 0x0000>;
		mtd-mac-address = <&factory 0x1000>;
		mtd-mac-address-increment = <2>;
		ieee80211-freq-limit = <2400000 2500000>;
	};
};

&gmac0 {
	mtd-mac-address = <&factory 0x1000>;
};

&switch0 {
	ports {
		port@0 {
			status = "okay";
			label = "lan4";
		};

		port@1 {
			status = "okay";
			label = "lan3";
		};

		port@2 {
			status = "okay";
			label = "lan2";
		};

		port@3 {
			status = "okay";
			label = "lan1";
		};

		port@4 {
			status = "okay";
			label = "wan";
			mtd-mac-address = <&factory 0x1000>;
			mtd-mac-address-increment = <1>;
		};
	};
};

&state_default {
	gpio {
		groups = "jtag", "uart2", "uart3", "wdt";
		function = "gpio";
	};
};

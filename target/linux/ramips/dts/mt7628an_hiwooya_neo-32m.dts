#include "mt7628an.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
        compatible = "hiwooya,neo-32m", "mediatek,mt7628an-soc";
        model = "HIWOOYA NEO (32M)";

	aliases {
		led-boot = &led_wifi;
		led-failsafe = &led_wifi;
		led-running = &led_wifi;
		led-upgrade = &led_wifi;
	};

	chosen {
		bootargs = "console=ttyS0,57600";
	};

	leds {
		compatible = "gpio-leds";

		led_wifi: wifi {
			label = "blue:wifi";
			gpios = <&gpio 44 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0pt";
		};
		led_lan: lan {
			label = "orange:lan";
			gpios = <&gpio 42 GPIO_ACTIVE_LOW>;
		};
		led_wan: wan {
			label = "orange:wan";
			gpios = <&gpio 43 GPIO_ACTIVE_LOW>;
		};
	};

	keys {
		compatible = "gpio-keys-polled";
		poll-interval = <20>;

		wps {
			label = "reset";
			gpios = <&gpio 38 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};
	};

	sound {
		compatible = "mediatek,mt762x-wm8960-machine";
		mediatek,platform = <&i2s>;
		mediatek,audio-codec = <&codec>;
		audio-routing =
			"Headphone", "HP_L",
			"Headphone", "HP_R",
			"Speaker", "SPK_LN",
			"Speaker", "SPK_LP",
			"Speaker", "SPK_RN",
			"Speaker", "SPK_RP",
			"Microphone", "MICB",
			"LINPUT1", "Microphone",
			"LINPUT2", "Line In",
			"RINPUT2", "Line In";
	};
};

&spi0 {
	status = "okay";
 
	pinctrl-names = "default";
	pinctrl-0 = <&spi_pins>, <&spi_cs1_pins>;
 
	m25p80@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <40000000>;
 
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;
 
			partition@0 {
				label = "u-boot";
				reg = <0x0 0x30000>;
				read-only;
			};
 
			partition@30000 {
				label = "u-boot-env";
				reg = <0x30000 0x10000>;
			};
			factory: partition@40000 {
				label = "factory";
				reg = <0x40000 0x10000>;
				read-only;
			};
 
			partition@50000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x50000 0x1fb0000>;
			};
		};
	};
 
	spidev@1 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "linux,spidev";
		reg = <1>;
		spi-max-frequency = <40000000>;
	};
};

&ethernet {
	nvmem-cells = <&macaddr_factory_28>;
	nvmem-cell-names = "mac-address";
};

&wmac {
	status = "okay";
	mediatek,mtd-eeprom = <&factory 0x0>;
};

&uart1 {
	status = "okay";
};

&gdma {
	status = "okay";
};

&i2c {
	status = "okay";

	codec: wm8960@1a {
		compatible = "wlf,wm8960";
		reg = <0x1a>;
		wlf,shared-lrclk;
		wlf,hp-cfg = <0 0 0>;
		wlf,gpio-cfg = <0 0>;
	};
};

&i2s {
	status = "okay";
};

&sdhci {
	status = "okay";
	mediatek,cd-low;
};

&pinctrl {
        state_default: pinctrl0 {
                gpio {
                        groups = "gpio", "p0led_an", "p1led_an", "wled_an", "wdt", "spis";
                        function = "gpio";
                };

		i2s {
			groups = "i2s";
			function = "i2s";
		};

		refclk {
			groups = "refclk";
			function = "refclk";
		};
        };
};

&esw {
        mediatek,portmap = <0x3>;
        mediatek,portdisable = <0x3c>;
};

&factory {
	compatible = "nvmem-cells";
	#address-cells = <1>;
	#size-cells = <1>;

	macaddr_factory_28: macaddr@28 {
		reg = <0x28 0x6>;
	};
};
